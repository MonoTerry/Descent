export (Vector2) var velocity = Vector2(0,0)
var ACCELERATION = 100;
var maxXVel = 4000
var jumpForce = 850;

var gravity = 15;

func _ready():
	
	pass # Replace with function body.

func _process(delta):
		
	
	if is_on_floor():
		velocity.y = 0;
	
	if Input.is_action_just_pressed(\"Up\"):
		if is_on_floor():
			velocity.y -= jumpForce;
	
	velocity.y += gravity
	move_and_slide(velocity,Vector2(0,-1));
	print(velocity)
	
	
